{
  "name": "git-board",
  "private": true,
  "version": "0.0.1",
  "description": "An aggregator of your git history",
  "author": "Andrea Duina, Francesco Zonaro",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "vue-tsc --noEmit && vite build",
    "start": "firebase serve --only hosting",
    "test-deploy": "pnpm run build && firebase hosting:channel:deploy stage --expires 1d",
    "deploy": "pnpm run build && firebase deploy --only hosting",
    "emulate": "firebase emulators:start"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.8",
    "@heroicons/vue": "^2.0.14",
    "axios": "^1.3.1",
    "chart.js": "^4.4.3",
    "chartjs-plugin-datalabels": "^2.2.0",
    "fast-levenshtein": "^3.0.0",
    "firebase": "^10.12.2",
    "octokit": "^4.0.2",
    "pinia": "^2.0.30",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/fast-levenshtein": "^0.0.4",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^3.2.0",
    "autoprefixer": "^10.4.13",
    "prettier": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.6.1",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.17.6",
    "vite": "^3.2.5",
    "vite-plugin-node-polyfills": "^0.7.0",
    "vue-tsc": "^0.40.13"
  }
}
